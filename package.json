{
  "name": "origraph",
  "private": true,
  "version": "0.3.0",
  "type": "module",
  "scripts": {
    "build-extension": "tsc -b && vite build --config vite.extension.config.ts && node ./src/scripts/generateChromeManifest.js",
    "build-library": "tsc -b && vite build --config vite.library.config.ts && node ./src/scripts/generateLibraryPackageJson.js && rimraf site/built-library && cp -r builds/library site/built-library",
    "build-storybook": "storybook build -o builds/storybook",
    "build-vocabulary": "tsx ./src/scripts/generateVocabulary.ts && prettier -w src/constants/vocabulary.ts",
    "build-site": "quarto render site",
    "build": "npm run build-library && npm run build-site && mv public/site/robots.txt public/ && tsc -b && vite build",
    "clean": "rimraf builds && rimraf coverage && rimraf site/.quarto && npm run test-post",
    "dev-app": "vite",
    "dev-site": "quarto preview site --no-browser",
    "lint": "eslint .",
    "preview": "npm run build && vite preview --outDir builds/website",
    "storybook": "storybook dev -p 6006 --no-open",
    "test-post": "rimraf --glob src/\\**/*.auto.story.test.ts*",
    "test-pre": "node ./src/scripts/generateStoryTests.js",
    "test-report": "npm run test-pre && vitest --coverage --ui --no-open && npm run test-post",
    "test": "npm run test-pre && vitest && npm run test-post"
  },
  "dependencies": {
    "@types/react-grid-layout": "^1.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@comunica/bindings-factory": "^3.3.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@floating-ui/react": "^0.27.3",
    "@monaco-editor/react": "^4.7.0-rc.0",
    "@rdfjs/types": "^2.0.1",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/lodash.isequal": "^4.5.8",
    "@types/n3": "^1.21.1",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "browser-level": "^2.0.0",
    "classnames": "^2.5.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.11.1",
    "globals": "^15.14.0",
    "immer": "^10.1.1",
    "jsdom": "^25.0.1",
    "lodash.isequal": "^4.5.0",
    "n3": "^1.23.1",
    "normalize.css": "^8.0.1",
    "prettier": "^3.4.2",
    "quadstore": "^14.0.0",
    "quadstore-comunica": "^5.0.0",
    "react-grid-layout": "^1.5.0",
    "readable-from-web": "^1.0.0",
    "rimraf": "^6.0.1",
    "semver": "^7.6.3",
    "storybook": "^8.4.7",
    "tsx": "^4.19.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.1",
    "use-immer": "^0.11.0",
    "uuid": "^11.0.3",
    "vite": "^6.2.0",
    "vite-multiple-assets": "^2.2.5",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^3.0.7",
    "wouter": "^3.3.5"
  }
}
